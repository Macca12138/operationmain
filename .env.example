# Google Sheets Configuration
# Copy this file to .env.local and fill in your values

# Your Google API Key (from Google Cloud Console)
NEXT_PUBLIC_GOOGLE_SHEETS_API_KEY=your_api_key_here

# Your Google Sheet ID or full URL
NEXT_PUBLIC_GOOGLE_SHEETS_ID=your_sheet_id_here

# The sheet name or range to load (default: Sheet1)
NEXT_PUBLIC_GOOGLE_SHEETS_RANGE=Sheet1

# Set to 'true' to auto-load Google Sheets data on startup
# Set to 'false' to show the upload screen first
NEXT_PUBLIC_AUTO_LOAD_GOOGLE_SHEETS=true
